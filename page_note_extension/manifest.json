{
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:3200/login_success"],
      "js": ["sendMsg.js"]
    }
  ],
  "permissions": [
    "tabs", 
    "http://*/*", 
    "https://*/*",
    "activeTab",
    "notifications"
  ],
   "options_page": "options.html",
  "name": "Page Note",
  "description": "页面笔记",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png"
    ,
    "default_popup": "login.html"
  }
}